<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene>
            <AnchorPane prefHeight="200" prefWidth="257.0">
            <children>
               <Label layoutX="14.0" layoutY="14.0" text="Would you like to delete an event or task?" />
               <TextField fx:id="type" layoutX="49.0" layoutY="59.0" />
               <Label layoutX="14.0" layoutY="31.0" text="Enter &quot;event&quot; or &quot;task&quot;: " />
               <Label layoutX="60.0" layoutY="109.0" text="Enter name of event/task:" />
               <TextField fx:id="typeName" layoutX="46.0" layoutY="135.0" />
               <Button fx:id="deleteEnterInfo" layoutX="102.0" layoutY="174.0" mnemonicParsing="false" text="Enter" />
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
